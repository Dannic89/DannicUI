<GuiXml> 
   <Controls>
      <TopLevelControl name="TargetHealthFrame" alpha="0" clampedToScreen="true" mouseEnabled="true" movable="true" hidden="false"> 
         <Dimensions x="390" y="42" />
         <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="320" offsetY="410"/>
         
         <Controls>
                  <Texture name="$(parent)Icon" textureFile="" level="3">
                     <Dimensions x="48" y="48" />
                     <Anchor point="BOTTOMLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="8" offsetY="12"/>
                  </Texture>

            <Backdrop name="$(parent)Backdrop" edgeColor="000000" layer="0" level="0" centerColor="000000" alpha="0.6"  hidden="false">
               <Dimensions x="390" y="42" />
               <Anchor point="TOPLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT"/>
               <Edge edgeSize="1" />
            </Backdrop>

            <Backdrop name="$(parent)Border" edgeColor="000000" layer="0" level="2" centerColor="00000000" alpha="1"  hidden="false">
               <Dimensions x="390" y="42" />
               <Anchor point="TOPLEFT"  relativeTo="$(parent)" relativePoint="TOPLEFT"/>
               <Edge edgeSize="1" />
            </Backdrop>

            <StatusBar name="$(parent)StatusBar" textureFile="dannicui/media/textures/Glass.dds" layer="0" level="1" color="bf171d" alpha="1" hidden="false">
               <Dimensions x="390" y="42" />
               <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0"/> 
               <Limits min="0" max="100" />
            </StatusBar>

            <Label name="$(parent)Label" font="ZoFontWinH1" color="ffffff" text="TargetName" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">			
               <Dimensions x="0" y="0" />
               <Anchor point="BOTTOMRIGHT"  relativeTo="$(parent)StatusBar" relativePoint="TOPRIGHT" offsetX="-4" offsetY="14"/>
            </Label>
            <Label name="$(parent)Health" font="ZoFontWinH1" color="ffffff" text="Health" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">			
               <Dimensions x="0" y="0" />
               <Anchor point="TOPLEFT"  relativeTo="$(parent)StatusBar" relativePoint="BOTTOMLEFT" offsetX="0" offsetY="0"/>
            </Label>
         </Controls>
      </TopLevelControl>

   </Controls>
</GuiXml>